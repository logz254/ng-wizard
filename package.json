{
  "name": "demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port=4228",
    "build": "ng build --aot",
    "lib:build": "rimraf dist && ng build ng-wizard --configuration production",
    "lib:themes": "mkdir -p dist/ng-wizard/themes && cp projects/ng-wizard/src/themes/*.css dist/ng-wizard/themes",
    "lib:files": "cp README.md dist/ng-wizard && cp LICENSE dist/ng-wizard",
    "lib:pack": "cd dist/ng-wizard && npm pack",
    "lib": "npm run lib:build && npm run lib:themes && npm run lib:files && npm run lib:pack",
    "test": "ng test ng-wizard",
    "lint": "ng lint ng-wizard"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "bootstrap": "^5.3.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "typescript": "~5.8.3"
  }
}